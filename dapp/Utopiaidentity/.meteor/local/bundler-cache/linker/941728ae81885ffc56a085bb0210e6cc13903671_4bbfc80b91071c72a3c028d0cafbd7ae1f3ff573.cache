ia').then(function (connected) {\n        if (!connected) return false;\n        scatter = ScatterJS.scatter;\n        var requiredFields = {\n          accounts: [network]\n        };\n        var eos = scatter.eos(network, Eos, eosOptions);\n        scatter.getIdentity(requiredFields).then(function () {\n          var acc = scatter.identity.accounts.find(function (x) {\n            return x.blockchain === 'eos';\n          });\n          var account = acc.name;\n          console.log(\"account \", account);\n          console.log(\"inlogin\");\n          localStorage.setItem(\"loginstatus\", JSON.stringify(true));\n          localStorage.setItem(\"username\", account);\n        }).catch(function (error) {\n          console.error(error);\n        });\n      });\n    } else {\n      ScatterJS.scatter.forgetIdentity().then(function () {\n        localStorage.setItem(\"loginstatus\", JSON.stringify(false));\n        localStorage.removeItem(\"username\");\n        console.log(\"logout\");\n      });\n    }\n  }\n});\n/////////////////////////////////////////////////////////////////////////////////\n\n}}},\"stylesheets\":{\"header.css\":function(require,exports,module){\n\n/////////////////////////////////////////////////////////////////////////////////\n//                                                                             //\n// imports/ui/stylesheets/header.css                                           //\n//                                                                             //\n/////////////////////////////////////////////////////////////////////////////////\n                                                                               //\nmodule.exports = require(\"meteor/modules\").addStyles(\n  \".container {\\n  width: 90%;\\n  max-width: 1140px;\\n  margin-left: auto;\\n  margin-right: auto;\\n  margin-top:2%;\\n}\\n.headSection {\\n  margin-bottom: 3%;\\n  background: #04030f;\\n}\\n/* .logo {\\n    max-width: 90px;\\n    height: 30px;\\n   